{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "importsNotUsedAsValues": "error",
    "jsx": "react-jsx",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@public/*": ["public/*"],
      "@shaders/*": ["src/shaders/*"]
    },
    "types": ["@astrojs/image/client"]
  }
}
